#include<stdio.h>

int main()
{
    int a,b,c,d;
    scanf("%d %d %d %d",&a,&b,&c,&d);
    if(d==0)
    {
    if(a>b && a>c)
    {
    printf("%d",a);
    }
    else if(b>a && b>c)
    {
    printf("%d",b);
    }
    else if(c>a && c>b)
    {
    printf("%d",c);
    }
    }
    if(d==1)
    {
    if(a<b && a<c)
    {
    printf("%d",a);
    }
    else if(b<a && b<c)
    {
    printf("%d",b);
    }
    else if(c<a && c<b)
    {
    printf("%d",c);
    }
    }
    
}
